<?xml version="1.0"?>
<block>
  <name>fsk_lecim_time_synchronizer_cc</name>
  <key>lpwan_fsk_lecim_time_synchronizer_cc</key>
  <category>[LPWAN]</category>
  <import>import lpwan</import>
  <make>lpwan.fsk_lecim_time_synchronizer_cc($preamble_len, $sps, $freq_dev, $symbol_rate)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>SHR length</name>
    <key>preamble_len</key>
    <type>int</type>
  </param>
  <param>
    <name>Sample per symbol</name>
    <key>sps</key>
    <type>int</type>
  </param>
  <param>
    <name>Frequency deviation</name>
    <key>freq_dev</key>
    <type>float</type>
  </param>
  <param>
    <name>Symbol rate</name>
    <key>symbol_rate</key>
    <type>float</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
